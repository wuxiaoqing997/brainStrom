page{
  width: 100%;
  height: 100%;
}
.container{
  width: 100%;
  height: 100%;
  padding: 0;
  display: flex;
  flex-direction: column;
  justify-content: flex-start;
  flex-wrap: nowrap;
  background-image: url('http://oofhxj2ur.bkt.clouddn.com/58b509ad4237d.jpg');
  background-position: center;
  background-origin: content-box;
  background-size: cover;
  background-repeat: no-repeat;
}
.left_lives{
  font-size: 30rpx;
  margin-left: 510rpx;
  margin-top: 50rpx;
  color: #303030;
}
.title_boder{
  margin-right: 20rpx;
  width: 10rpx;
  height: 100%;
  background-color: rgba(0, 0, 0, 0.2);
}
.title{
  display: flex;
  flex-wrap: nowrap;
  margin-top: 30rpx;
  margin-right: 399rpx;
  padding-left: 0;
  width: 38%;
  height: 60rpx;
  line-height: 60rpx;
  background-color: rgba(196, 196, 196, 0.5);
  color: rgba(255, 255, 255, 0.8);
  border-radius: 0 6rpx 6rpx 0;
}
.result_wrap{
  margin-top: 70rpx;
  width: 100%;
  height: 530rpx;
  display: flex;
  flex-direction: row;
  justify-content: flex-start;
  flex-wrap: wrap;
  margin-left: 70rpx;
}
.result_item{
  width: 80rpx;
  height: 80rpx;
  line-height: 80rpx;
  text-align: center;
  vertical-align: middle;
  border-radius: 50%;
  background-color: rgba(119,136,153,0.5);
  margin-right: 50rpx;
  margin-bottom: 25rpx; 
  flex-shrink: 0;
}
.result_error{
  background-color: rgba(255,64,64, 0.8);
}
.bottom{
  margin-top: 30rpx;
  width: 100%;
  height: 60rpx;
  line-height: 60rpx;
  display: flex;
  justify-content: flex-end;
  padding-right: 60rpx;
  font-size: 36rpx;
}
.once_again{
  width: 180rpx;
  height: 60rpx;
  line-height: 60rpx;
  background-color: rgba(196, 196, 196, 0.5);
  color: rgba(255, 255, 255, 0.8);
  border-radius: 10rpx;
  text-align: center;
}
.submit_result{
  margin-left: 10rpx;
  width: 220rpx;
  height: 60rpx;
  line-height: 60rpx;
  background-color: rgba(196, 196, 196, 0.5);
  color: rgba(255, 255, 255, 0.8);
  border-radius: 10rpx;
  text-align: center;
}
.swiper_item{
  width: 698rpx;
  height: 530rpx;
}
.swiper_item_wrap{
  display: flex;
  flex-wrap: wrap;
  padding-top: 30rpx;
  padding-left: 40rpx;
}
.result_tip{
  margin-top: 30rpx;
  margin-left: 200rpx;
  font-size: 32rpx;
    color: #303030;
}